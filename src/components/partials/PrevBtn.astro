---
import { btnClasses } from '~/lib';

interface Props {
  url: string;
}

const label = "paginate prev button"

const { url } = Astro.props as Props

---

<a href={url} aria-label={label} class:list={btnClasses}>
  <svg viewBox="0 0 24 24" fill="currentColor" class="w-6 h-6 sm:w-4 sm:h-4">
    <path fill-rule="evenodd" d="M7.72 12.53a.75.75 0 010-1.06l7.5-7.5a.75.75 0 111.06 1.06L9.31 12l6.97 6.97a.75.75 0 11-1.06 1.06l-7.5-7.5z" clip-rule="evenodd" />
  </svg>
</a> 